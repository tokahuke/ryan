{
  "name": "ryan-syntax-highlighting",
  "displayName": "Ryan Syntax Highlighting",
  "description": "VSCode support for the Ryan configuration language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ryan",
      "aliases": ["Ryan", "ryan"],
      "extensions": [".ryan",".ryans"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ryan",
      "scopeName": "source.ryan",
      "path": "./syntaxes/ryan.tmLanguage.json"
    }]
  }
}
