{
  "name": "ryan-syntax-highlighting",
  "displayName": "Ryan Syntax Highlighting",
  "description": "VSCode support for the Ryan configuration language",
  "publisher": "PedroBArruda",
  "repository": {
    "type": "git",
    "url": "https://github.com/tokahuke/ryan"
  },
  "license": "MIT",
  "version": "0.1.0",
  "icon": "mascot.jpg",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ryan",
        "aliases": [
          "Ryan",
          "ryan"
        ],
        "extensions": [
          ".ryan",
          ".ryans"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ryan",
        "scopeName": "source.ryan",
        "path": "./syntaxes/ryan.tmLanguage.json"
      }
    ]
  }
}
